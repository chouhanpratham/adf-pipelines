{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-demo-pratham"
		},
		"AdventureWorksDb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AdventureWorksDb'"
		},
		"BlobStorageForProducts_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorageForProducts'"
		},
		"BlobStorageforCustomer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorageforCustomer'"
		},
		"CustomerBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CustomerBlobStorage'"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AdventureWorksDb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dbpratham.database.windows.net"
		},
		"AdventureWorksDb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "adfdatabase"
		},
		"AdventureWorksDb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "PrathamChouhan"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-demo-pratham_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdventureWorksDb_password": {
						"value": "[parameters('AdventureWorksDb_password')]"
					},
					"BlobStorageForProducts_connectionString": {
						"value": "[parameters('BlobStorageForProducts_connectionString')]"
					},
					"BlobStorageforCustomer_connectionString": {
						"value": "[parameters('BlobStorageforCustomer_connectionString')]"
					},
					"CustomerBlobStorage_connectionString": {
						"value": "[parameters('CustomerBlobStorage_connectionString')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AdventureWorksDb_properties_typeProperties_server": {
						"value": "[parameters('AdventureWorksDb_properties_typeProperties_server')]"
					},
					"AdventureWorksDb_properties_typeProperties_database": {
						"value": "[parameters('AdventureWorksDb_properties_typeProperties_database')]"
					},
					"AdventureWorksDb_properties_typeProperties_userName": {
						"value": "[parameters('AdventureWorksDb_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}